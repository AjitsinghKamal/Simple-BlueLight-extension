{
	"name":"Simple BlueLight",
	"version":"0.0.1",
	"description":"",
	"manifest_version":2,
	"permissions":[ "http://*/*", "https://*/*"],
	"content_scripts":[
		{
		"matches": [ "*://*/*"],
        "js": ["myScript.js"],
        
        "css":["filter.css"]
    }
  ],
  "web_accessible_resources":[
  	"myScript.js","filter.css"]
  
}
